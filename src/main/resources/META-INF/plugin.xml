<idea-plugin>
    <id>1.1</id>
    <name>Convent JSON/SQL to GO Struct</name>
    <vendor email="github.haoliu@gmail.com" url="">NULL</vendor>

    <description><![CDATA[
    Convert JSON or SQL Create TABLE to Go Struct.<br>

    <em>Select JSON/SQL right click and SELECT "Convert JSON/SQL To GO"</em>
    <p>enjoy.</p>
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="RightClickJSON" class="ConvertJSONAction" text="Convent JSON to GO" description="xxx">
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
        </action>
        <action id="RightClickSQL" class="ConvertSQLAction" text="Convent SQL to GO" description="xxx">
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
        </action>
    </actions>
</idea-plugin>